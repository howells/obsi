{
  "name": "obsi",
  "version": "1.3.0",
  "description": "Interactive Obsidian vault CLI with TUI",
  "type": "module",
  "bin": {
    "obsi": "./bin/cli.cjs"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "postinstall": "node bin/postinstall.cjs"
  },
  "keywords": [
    "obsidian",
    "notes",
    "capture",
    "claude-code",
    "cursor",
    "ai-coding"
  ],
  "author": "Daniel Howells",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/howells/obsi.git"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "fuse.js": "^7.1.0",
    "ink": "^6.6.0",
    "react": "^19.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.17",
    "ink-testing-library": "^4.0.0",
    "vitest": "^4.0.17"
  }
}
